{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",  
        "include": ["."],                      
        "exclude": ["."] 
    },
    "plugins": ["plugins/markdown","node_modules/jsdoc-tsimport-plugin/index.js"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./jsdocs/",
      "readme": "README.md"
    }
  }